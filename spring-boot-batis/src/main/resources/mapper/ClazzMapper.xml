  <select id="getClazzOneToMany" resultMap="clazzMap" parameterType="int">
    select c.clazz_id,c.clazz_name,s.student_id,s.student_name,s.hometown
    from t_clazz c,
         t_student s
    where c.clazz_id = #{teacherId}
      and c.clazz_id = s.clazz_id
  </select>
